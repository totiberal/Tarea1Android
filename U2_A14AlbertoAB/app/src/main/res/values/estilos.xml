<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="estiloLineas" parent="@android:style/TextAppearance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/lineas</item>
        <item name="android:background">@color/negro</item>
    </style>
    <style name="estiloRB" parent="@android:style/TextAppearance">
        <item name="android:layout_width">@dimen/rboton</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="todoWrap" parent="@android:style/TextAppearance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="todoMatch" parent="@android:style/TextAppearance">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>
    <style name="matchWrap" parent="@android:style/TextAppearance">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


</resources>